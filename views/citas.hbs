<table border="solid">
    <h2>LISTADO DE CITAS</h2>
    <thead>
        <tr>
            <th>Cedula Paciente</th>
            <th>Cedula Medico</th>
            <th>Fecha</th>
            <th>Consultorio</th>
            <th>Opciones</th>
        </tr>
    </thead>
    <tbody>
       {{#each citas}}
       <tr>
        <td>{{paciente}}</td>
        <td>{{medico}}</td>
        <td>{{formatearFecha fecha}}</td>
        <td>{{consultorio}}</td>
        <td><a class="eliminar" href="citas/eliminar/{{id}}">Eliminar</a></td>
       </tr>
       {{/each}}
    </tbody>
</table>
<a href="/citas/agregar">Agregar cita medica</a>